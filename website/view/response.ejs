<html>
    <body>
        <% for(var i=0; i<results.length; i++){ %>
            <div style="background-color:lightblue">
                <p>username: <%= results[i].userName %></p>
                <p>password: <%= results[i].password %></p>
                <p>email: <%= results[i].email %></p>
            </div>
        <% } %>
        <form method="post" action="/" enctype="application/x-www-form-urlencoded">
            <fieldset>
                <div><input id="getBack" type="submit" value="Get back to main page" /></div>
            </fieldset>
        </form>
    </body>
</html>